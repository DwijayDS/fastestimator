pipeline{
    agent any
    stages{
        stage('test'){
            steps{
                withCredentials([string(credentialsId: 'pypi_password', variable: 'PYPI_PWD')]) {
                    sh '''
                        echo hello
                        echo ${PYPI_PWD}
                    '''                   
                }
            }         
        }
    }
}        
    